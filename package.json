{
	"name": "@accuser/svelte-store-array",
	"version": "0.4.1",
	"description": "Higher-order Svelte stores for array manipulation",
	"keywords": [
		"array",
		"higher-order",
		"store",
		"svelte"
	],
	"homepage": "https://github.com/accuser/svelte-store-array/README.md",
	"bugs": {
		"url": "https://github.com/accuser/svelte-store-array/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Matthew Gibbons",
		"url": "https://github.com/accuser"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/accuser/svelte-store-array.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest run",
		"test:cov": "vitest run --coverage"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.84",
		"@sveltejs/kit": "1.0.0-next.522",
		"@sveltejs/package": "1.0.0-next.5",
		"@typescript-eslint/eslint-plugin": "^5.40.1",
		"@typescript-eslint/parser": "^5.40.1",
		"@vitest/coverage-c8": "^0.24.3",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4",
		"vite": "^3.1.8",
		"vitest": "^0.24.3"
	},
	"type": "module"
}
